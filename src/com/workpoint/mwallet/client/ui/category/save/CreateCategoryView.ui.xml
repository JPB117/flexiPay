<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:d='urn:import:com.workpoint.mwallet.client.ui.component' xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:a='urn:import:com.workpoint.mwallet.client.ui.component.autocomplete'
	xmlns:t='urn:import:com.workpoint.mwallet.client.ui.component.tabs'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<!-- Issues -->
		<d:IssuesPanel ui:field="issues">
		</d:IssuesPanel>

		<g:HTMLPanel>
			<fieldset>
				<div class="control-group">
					<div class="control-label" ui:field="divCategoryName">Category Name:</div>
					<div class="controls">
						<g:TextBox ui:field="txtCategoryName" styleName="input-xlarge"></g:TextBox>
						<span class="mandatory" title="Required Field">*</span>
					</div>
				</div>

				<div class="control-group">
					<div class="control-label" ui:field="divActivityName">Category Type:</div>
					<div class="controls">
						<d:DropDownList ui:field="lstCategoryType"></d:DropDownList>
						<span class="mandatory" title="Required Field">*</span>
					</div>
				</div>

			</fieldset>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder>

