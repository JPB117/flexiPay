<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:d="urn:import:com.workpoint.mwallet.client.ui.component">

	<g:HTMLPanel styleName="tr" ui:field="row">
		<g:HTMLPanel styleName="td" ui:field="divCustNames">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divPhone">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divAmount">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divReferenceId">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divDate">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divTills">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divStatus">
			<b:Popover heading="Status"
				ui:field="popoverStatus" placement="LEFT">
				<g:InlineLabel styleName="label label-default"
					ui:field="spnStatus">posted</g:InlineLabel>
			</b:Popover>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 