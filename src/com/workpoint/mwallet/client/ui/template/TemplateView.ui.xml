<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	xmlns:d='urn:import:com.workpoint.mwallet.client.ui.component' xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:programs='urn:import:com.workpoint.mwallet.client.ui.template'
	xmlns:table='urn:import:com.workpoint.mwallet.client.ui.template.table'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="activities-page">
		<g:HTMLPanel styleName="full-page" ui:field="divMainContainer">
			<g:HTMLPanel styleName="content-top" ui:field="divContentTop">
				<div class="span3">
					<d:ActionLink ui:field="aCreate" styleName="btn btn-primary"
						dataToggle="tooltip">
						Create Template
					</d:ActionLink>
				</div>

				<div class="span3">
					<d:ActionLink ui:field="aEdit" styleName="btn btn-default"
						dataToggle="tooltip" dataPlacement="bottom" title="Edit">
						<i class="icon-pencil icon-button"></i>
					</d:ActionLink>
				</div>

				<div class="span3">
					<d:ActionLink ui:field="aDelete" styleName="btn btn-danger"
						dataToggle="tooltip" dataPlacement="bottom" title="Delete">
						<i class="icon-trash icon-button"></i>
					</d:ActionLink>
				</div>
			</g:HTMLPanel>

			<g:HTMLPanel ui:field="divMiddleContent" styleName="row-fluid">
				<div ui:field="divTab1Content" class="tab-pane fade in active">
					<table:TemplateTable ui:field="tblView">
					</table:TemplateTable>
				</div>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>