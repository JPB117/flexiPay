<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:d='urn:import:com.workpoint.mwallet.client.ui.component' xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<fieldset>

			<!-- <div class="control-group hide"> <div class="control-label" ui:field="divTill">Till 
				Code:</div> <div class="controls"> <g:TextBox ui:field="" styleName="input-xlarge"></g:TextBox> 
				<span class="mandatory" title="Required Field">*</span> </div> </div> -->

			<div class="control-group">
				<div class="control-label" ui:field="divMessage">Message</div>
				<div class="controls">
					<g:TextBox ui:field="txtmessage" styleName="input-xlarge"></g:TextBox>
				</div>
			</div>

			<div class="control-group">
				<div class="control-label" ui:field="divType">Type</div>
				<div class="controls">
					<g:TextBox ui:field="txtType" styleName="input-xlarge"></g:TextBox>
				</div>
			</div>

			<div class="control-group">
				<div class="control-label" ui:field="divName">Name</div>
				<div class="controls">
					<g:TextBox ui:field="txtName" styleName="input-xlarge"></g:TextBox>
				</div>
			</div>

			<div class="control-group">
				<div class="control-label" ui:field="divIsDefault">Is Default</div>
				<div class="controls">
					<g:CheckBox ui:field="txtisDefault" styleName="input-xlarge"></g:CheckBox>

				</div>
			</div>

			<div class="control-group">
				<div class="control-label" ui:field="divTill">Till Model</div>
				<div class="controls">
					<g:TextBox ui:field="txttillModel_Id" styleName="input-xlarge"></g:TextBox>
				</div>
			</div>

			<div class="control-group">
				<div class="control-label" ui:field="divEnabled">Enabled:</div>
				<div class="controls">
					<g:CheckBox ui:field="chckEnable" styleName="input-xlarge"></g:CheckBox>
				</div>
			</div>

			<div class="control-group">
				<label class="control-label">
					<b>Category:</b>
				</label>
				<d:DropDownList ui:field="lstCategoryType"></d:DropDownList>
			</div>
		</fieldset>
	</g:HTMLPanel>
</ui:UiBinder> 