<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:a='urn:import:com.workpoint.mwallet.client.ui.component.autocomplete'
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<fieldset>
			<div class="control-group">
				<div class="control-label" ui:field="divActivityName">Till Owner:</div>
				<div class="controls">
					<a:AutoCompleteField ui:field="Owners" />
				</div>
			</div>

			<div class="control-group">
				<div class="control-label" ui:field="divTill">Till Operator:</div>
				<div class="controls">
					<a:AutoCompleteField ui:field="Operators" />
				</div>
			</div>

			<div class="control-group">
				<div class="control-label" ui:field="divPhone">Till Acquirer:</div>
				<div class="controls">
					<a:AutoCompleteField ui:field="acquirers" />
				</div>
			</div>
		</fieldset>
	</g:HTMLPanel>
</ui:UiBinder> 