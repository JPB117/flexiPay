<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d='urn:import:com.workpoint.mwallet.client.ui.component'>

	<g:HTMLPanel>
		<div class="header row-fluid">
			<div class="span9 title" ui:field="divHeader">
				<g:FocusPanel ui:field='panelTitle'>
					<g:HTMLPanel styleName="row-fluid">
						<g:HTMLPanel styleName="title-container span3">
							<h4 ui:field="spnTitle" class="title">Tills</h4>
						</g:HTMLPanel>
					</g:HTMLPanel>
				</g:FocusPanel>
			</div>
			
			<g:HTMLPanel styleName="span3 budget" ui:field="divBudget">
				<div class="span12">
					<div class="bold muted helper-font-small">TOTAL TILLS</div>
					<div>
						<span class="bold" ui:field="spnTillTotal" title="Total Number of Tills"></span>
					</div>
				</div>
			</g:HTMLPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 